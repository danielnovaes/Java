import entities.Conta;
import entities.ContaNegocio;
import entities.ContaPoupanca;

public class Program {

	public static void main(String[] args) {
		
		Conta conta = new Conta(1001, "Daniel", 0.0);
		ContaNegocio contanegocio = new ContaNegocio(1002, "Jose", 0.0, 500.0);
		
		// Upcasting 
		
		Conta conta2 = contanegocio;
		Conta conta3 = new ContaNegocio(1003, "Carlos", 0.0, 300.0);
		Conta conta4 = new ContaPoupanca(1004, "Eduardo", 0.0, 0.01);
		
		
	}

}
