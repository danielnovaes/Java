package entities;

import java.util.Date;

import entities.enums.StatusOrdem;

public class Pedido {
	
	private Integer id;
	private Date momento;
	private StatusOrdem status;
	
	public Pedido() {
		
	}

	public Pedido(Integer id, Date momento, StatusOrdem status) {
		this.id = id;
		this.momento = momento;
		this.status = status;
	}
	
}
